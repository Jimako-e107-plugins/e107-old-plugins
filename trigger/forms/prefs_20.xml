      </accesstable>

      <accesstable name="trigger_fup_download_visible" label="TRIGGER_LAN_PREFS_20_4" prompt="TRIGGER_LAN_PREFS_20_4p" help="TRIGGER_LAN_PREFS_20_4h" class="tbox">
