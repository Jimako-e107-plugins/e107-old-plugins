<?xml version="1.0" ?>
<!-- 1st tag in the XML file must be e107HelperForm -->
<e107HelperForm version="1.0">

   <!--
   We're defining an input form so use the 'form' tag.
   The 'name' attribute can be anything you want but should only be made up of letters, numbers and underscore.
   The 'type' attribute indicates what type of form this is, in this case it's an e107 preferences form
   to allow a user to manage preferences for a plugin
   -->
   <form name="e107helpers_developer_admin_prefs_form" type="HELPER_FORM_TYPE_E107_PREF">

      <div name="ehd_header1" text="EHD_LAN_ADMIN_MENU_12">
         <style name="font-weight">bold</style>
         <style name="text-align">center</style>
      </div>

      <!--  -->
      <list name="ehd_list" label="EHD_LAN_ADMIN_PREFS_11_0" prompt="EHD_LAN_ADMIN_PREFS_11_1" help="EHD_LAN_ADMIN_PREFS_11_2" class="tbox">
         <default var="e107pref"/>
         <value key="10">EHD_LAN_ADMIN_PREFS_11_VALUE_0</value>
         <value key="21">EHD_LAN_ADMIN_PREFS_11_VALUE_1</value>
      </list>

      <!--  -->
      <list name="ehd_list2" label="EHD_LAN_ADMIN_PREFS_19_0" prompt="EHD_LAN_ADMIN_PREFS_19_1" help="EHD_LAN_ADMIN_PREFS_19_2" class="tbox">
         <default var="e107pref"/>
         <value>EHD_LAN_ADMIN_PREFS_19_VALUE_0</value>
         <value>EHD_LAN_ADMIN_PREFS_19_VALUE_1</value>
         <attribute name="size">3</attribute>
         <style name="width">75%</style>
      </list>

      <!--  -->
      <list name="ehd_list3" label="EHD_LAN_ADMIN_PREFS_16_0" prompt="EHD_LAN_ADMIN_PREFS_16_1" help="EHD_LAN_ADMIN_PREFS_16_2" class="tbox">
         <default var="e107pref"/>
         <value key="0">EHD_LAN_ADMIN_PREFS_16_VALUE_0</value>
         <value key="1">EHD_LAN_ADMIN_PREFS_16_VALUE_1</value>
         <attribute name="multiple">true</attribute>
         <attribute name="size">3</attribute>
      </list>

      <!--  -->
      <accesstable name="ehd_accesstable" label="EHD_LAN_ADMIN_PREFS_01_0" prompt="EHD_LAN_ADMIN_PREFS_01_1" help="EHD_LAN_ADMIN_PREFS_01_2" class="tbox">
         <default var="e107pref"/>
      </accesstable>

      <!--  -->
      <dirlist name="ehd_dir" label="EHD_LAN_ADMIN_PREFS_09_0" prompt="EHD_LAN_ADMIN_PREFS_09_1" help="EHD_LAN_ADMIN_PREFS_09_2" class="tbox">
         <default var="e107pref"/>
         <dir>e_FILE</dir>
      </dirlist>

      <!--  -->
      <filelist name="ehd_file" label="EHD_LAN_ADMIN_PREFS_24_0" prompt="EHD_LAN_ADMIN_PREFS_24_1" help="EHD_LAN_ADMIN_PREFS_24_2" class="tbox">
         <default var="e107pref"/>
         <dir>e_FILE</dir>
      </filelist>

      <!--  -->
      <table name="ehd_table" label="EHD_LAN_ADMIN_PREFS_13_0" prompt="EHD_LAN_ADMIN_PREFS_13_1" help="EHD_LAN_ADMIN_PREFS_13_2" class="tbox">
         <default var="e107pref"/>
         <value key="table">user</value>
         <value key="columns">*</value>
         <value key="order">user_name ASC</value>
         <value key="limit">0,10</value>
      </table>

   </form>

</e107HelperForm>